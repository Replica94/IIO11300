        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:H5movies"
        mc:Ignorable="d"
        Title="Valikko" Height="150" Width="350" FontFamily="Showcard gothic">
    <Grid>
        <StackPanel>
            <TextBlock>Elokuvagalleria</TextBlock>
